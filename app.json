{
  "expo": {
    "name": "StoreFlow",
    "slug": "storeflow",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "translucent": false
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ktshacx.storeflow",
      "infoPlist": {
        "UIDeviceFamily": [1],
        "NSCameraUsageDescription": "This app requires camera access to allow users to scan their barcoder. Photos will not be saved privately or shared publicly."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ktshacx.storeflow"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "fb71a58f-9d7d-455a-961c-93eb98bcba47"
      }
    },
    "owner": "ktshacx"
  }
}
